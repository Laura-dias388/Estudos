{
  "name": "teste_back",
  "version": "1.0.0",
  "description": "projeto para aprender a testar com mocha, sinon e chai",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "test": "mocha __tests__/**/*.{test,spec}.js --exit",
    "coverage": "nyc --exclude src/database/models/index.js npm test",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "prestart": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "restart": "npx sequelize-cli db:drop && npm run prestart"
  },
  "keywords": [],
  "author": "Laura",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mysql2": "^3.5.1",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0"
  }
}
